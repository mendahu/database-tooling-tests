  "license": "ISC",
  "description": "",
  "dependencies": {
    "@pgtyped/runtime": "^2.3.0",
    "express": "^4.21.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@pgtyped/cli": "^2.3.0",
    "dotenv-cli": "^7.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.10",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  }
}
